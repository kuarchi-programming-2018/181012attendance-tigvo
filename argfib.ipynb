    "def fib(n):\n",
    "    x=0\n",
    "    y=1\n",
    "    for i in  range(n):\n",
    "        z=x+y\n",
    "        x=y\n",
    "        y=z\n",
    "    return x\n",
    "def argfib(m):\n",
    "    i=0\n",
    "    while m>=fib(i):\n",
    "        if fib(i+1)>m:\n",
    "            return fib(i)\n",
    "        else:\n",
    "            i+=1\n",
    "m=2018\n",
    "print(argfib(m))"
